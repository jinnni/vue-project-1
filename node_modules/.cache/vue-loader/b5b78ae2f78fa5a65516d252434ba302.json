     title: {\n                        text: '极坐标双数值轴'\n                    },\n                    legend: {\n                        data: ['line']\n                    },\n                    xAxis: { type: 'category' },\n                    yAxis: {},\n                    tooltip: {\n                        trigger: 'axis',\n                        axisPointer: {\n                            type: 'cross'\n                        }\n                    },\n                    dataset:{\n                        source: []\n                    },\n                    series: [{type:'bar'}],\n                },   \n            }\n        },\n        mounted() {\n            const pageInfo = this.pageInfo;\n            this.$store.dispatch('campaignBrand', pageInfo).then(resp =>{\n                let list = resp.data.content;\n                if (list.length > 0) {\n                    list.forEach(item => {\n                        this.brandList.dataset.source.push([item.name, item.campaignMembersCount])\n                    }); \n                }\n            });\n        }\n    };\n</script>"]}]}