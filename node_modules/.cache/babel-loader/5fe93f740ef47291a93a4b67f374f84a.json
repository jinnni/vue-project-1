: function () {\n        this.$store.dispatch('logout')\n        .then(() => {\n          this.$router.push('/login')\n        })\n      }\n    },\n    created: function () {\n      this.$http.interceptors.response.use(undefined, function (err) {\n        return new Promise(function ( ) {\n          if (err.status === 401 && err.config && !err.config.__isRetryRequest) {\n            this.$store.dispatch('logout')\n          }\n          throw err;\n        });\n      });\n    }\n  }\n</script>\n"],"sourceRoot":"src"}]}